{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "target": "es6",
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "downlevelIteration": true,
    "outDir": "bin"
  },
  "include": ["src"]
}